{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types"
, "pages/contact.mdx", "pages/corpus.mdx", "pages/_app.mdx", "pages/evaluation.mdx", "pages/index.mdx", "pages/resources.mdx", "pages/results.mdx", "pages/schedule.mdx", "pages/submission.mdx", "pages/tasks.mdx", "pages/application.mdx", "pages/_document.tsx", "pages/overview.mdx"  ],
  "exclude": [
    "node_modules"
  ]
}
